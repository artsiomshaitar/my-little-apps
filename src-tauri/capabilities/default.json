{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "opener:default",
    "shell:default",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-kill",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-load",
    "sql:allow-close",
    "dialog:default",
    "dialog:allow-open"
  ]
}
